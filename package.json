{
  "name": "zeta-json",
  "displayName": "Zeta Json",
  "description": "Zeta JSON Tools to Visual Studio Code",
  "version": "0.0.2",
  "publisher": "Hermosilla",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/zeta-vscode-json.png",
  "activationEvents": [
    "onCommand:zeta-json.toJsonSchema"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "when": "resourceLangId == jsonc || resourceLangId == json",
          "command": "zeta-json.toJsonSchema",
          "alt": "zeta-json.toJsonSchema",
          "group": "2_workspace"
        }
      ]
    },
    "commands": [
      {
        "command": "zeta-json.toJsonSchema",
        "title": "z Create JSON Schema"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hermosilla/vsc-zeta-json-tools.git"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.44.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "to-json-schema": "^0.2.5",
    "strip-json-comments": "^3.1.0"
  }
}
